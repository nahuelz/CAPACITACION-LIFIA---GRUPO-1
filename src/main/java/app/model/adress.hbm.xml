<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
		<hibernate-mapping package="app.model">
			<class name="Adress" table="ADRESS">
				<id name="idAdress" column="ID_ADRESS" type="int">
					<generator class="native" />
				</id>
				
				<property name="country" column="COUNTRY"></property>
				
				<property name="state" column="STATE"></property>
				
				<property name="city" column="CITY"></property>
				
				<property name="street" column="STREET"></property>
				
				<property name="altitude" column="ALTITUDE"></property>
				
				<property name="departament" column="DEPARTAMENT"></property>
				
			</class>
		
		</hibernate-mapping>